{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "custom-blocks/custom-carousel",
	"version": "0.1.0",
	"title": "Custom Carousel",
	"category": "widgets",
	"icon": "smiley",
	"description": "Custom carousel.",
	"example": {},
	"supports": {
		"html": false
	},
    "attributes": {
        "categories": {
            "type": "array",
            "default": [],
            "source": "query",
            "selector": ".category-item",
            "query": {
                "image": {
                    "type": "string",
                    "source": "attribute",
                    "selector": "img",
                    "attribute": "src"
                },
                "name": {
                    "type": "string",
                    "source": "text",
                    "selector": ".category-name"
                }
            }
        }
    },
	"textdomain": "custom-blocks",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"viewScript": "file:./view.js",
    "render": "file:./render.php"
}